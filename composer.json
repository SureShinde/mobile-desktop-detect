{
    "name": "sureshinde/mobile-desktop-detect",
    "description": "Desktop/mobile user agent parser with support for Laravel, based on MobiledDetect",
    "keywords": ["laravel", "useragent", "agent", "user agent", "browser", "platform", "mobile", "desktop"],
    "homepage": "https://github.com/sureshinde/mobile-desktop-detect",
    "license" : "MIT",
    "version": "3.0.3",
    "authors": [
        {
            "name": "Suresh Shinde",
            "homepage": "https://sureshinde.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "mobiledetect/mobiledetectlib": "^4.8",
        "jaybizzle/crawler-detect": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "SureShinde\\MobileDesktopDetect\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        },
        "laravel": {
            "providers": [
                "SureShinde\\MobileDesktopDetect\\AgentServiceProvider"
            ],
            "aliases": {
                "Agent": "SureShinde\\MobileDesktopDetect\\Facades\\Agent"
            }
        }
    },
    "suggest": {
        "illuminate/support": "Required for laravel service providers"
    }
}
